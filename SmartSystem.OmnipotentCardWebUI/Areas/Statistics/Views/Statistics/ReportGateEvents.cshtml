@using SmartSystem.OmnipotentCardWebUI.Models
@{
    ViewBag.Title = "ReportGateEvents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.ReferenceScriptHtml("/Scripts/statistics/common.js")
@Html.ReferenceScriptHtml("/Scripts/statistics/reportgateevents.js")
<div class="easyui-layout" style="height:500px;width:500px" fit="true">
<div region="center" title="通道事件" style="padding:2px; ">
    <div style="margin:3px 0px 3px 5px">    
        <div style="margin-bottom: 5px">   
        <table>
            <tr>
                <td align="right">车场名称：</td><td><input id="selectParks" name="ParkingId" class="easyui-combobox"  style=" width:150px"/></td>
                <td align="right">事件名称：</td><td><select id="selectEvent" style="width: 150px; " class="my_select"><option value="-1">全部</option></select></td>
                <td align="right">车辆类型：</td><td><select id="selectCarType" style="width: 150px; " class="my_select"><option value="-1">全部</option></select></td>
                <td align="right">卡片类型：</td><td><select id="selectCardType" style="width: 150px;" class="my_select"><option value="-1">全部</option></select></td>
                <td align="right">进出方向：</td><td><select id="selectInOut" style="width: 150px;" class="my_select"><option value="-1">全部</option><option value="1">进</option><option value="2">出</option></select></td>
            </tr>
              <tr>
                <td align="right">通道名称：</td><td><select id="selectGate" style="width: 150px; " class="my_select"><option value="-1">全部</option></select></td>
                <td align="right">车牌号码：</td><td><input class="my_textbox" style="width: 150px; " id="txtPlateNumber" /> </td>
                <td align="right">开始时间：</td><td><input class="easyui-datetimebox" type="text" id="txtEntranceTime" style="width:150px;height:25px; line-height:25px"  /></td>
                <td align="right">结束时间：</td><td><input class="easyui-datetimebox" type="text" id="txtExitTime" style="width:150px;height:25px; line-height:25px"  /> </td>
                <td></td><td><a href="javascript:void(0)" id="btnQueryData" class="easyui-linkbutton" style="width: 80px;height: 28px;">查询</a></td>
            </tr>
        </table>
        </div>

    </div>
    <div style="height: 88%;">        
        <table fit="true" id="tableListBox">
        </table>
    </div>
</div>
</div>
<div id="DivShowImage" style="display: none">
    <form id="DivShowImageForm" class="easyui-form">
    <div style="padding: 10px; background-image='../../Content/images/amg_3.png'">
        <table>
        <tr><td style=" text-align:center"><small>通道图片</small></td></tr>
        <tr><td><img id="EntranceImage" width="380px" height="280px"/></td></tr>
        </table>
    </div>
    </form>
</div>

