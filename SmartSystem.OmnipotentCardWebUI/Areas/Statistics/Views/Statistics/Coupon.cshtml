@using SmartSystem.OmnipotentCardWebUI.Models
@{
    ViewBag.Title = "商家优免";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.ReferenceScriptHtml("/Scripts/statistics/common.js")
@Html.ReferenceScriptHtml("/Scripts/statistics/statisticscoupon.js")

<div class="easyui-layout" style="height:500px;width:500px" fit="true">
<div region="center" title="商家优免" style="padding:2px; ">
    <div style="margin:3px 0px 3px 5px">    
        <div style="margin-bottom: 5px">   
        <table>
            <tr>
                <td align="right">车场名称：</td><td><input id="selectParks" name="ParkingId" class="easyui-combobox"  style=" width:150px"/></td>
                <td align="right">状态：</td><td><select id="selectStatus" style="width: 150px; " class="my_select"><option value="-1">全部</option><option value="0">正常</option><option value="1">使用中</option><option value="2">已结算</option><option value="3">已作废</option></select> </td>
                <td align="right">商家：</td><td><select id="selectSeller" style="width: 150px; " class="my_select"></select></td>
                <td align="right">优免编号：</td><td><input class="my_textbox" style="width: 150px; " id="txtCouponNo" /> </td>
            </tr>
             <tr>
                <td align="right">开始时间：</td><td><input class="easyui-datetimebox" type="text" id="txtStartTime" style="width:150px;height:25px; line-height:25px"  /></td>
                <td align="right">结束时间：</td><td><input class="easyui-datetimebox" type="text" id="txtEndTime" style="width:150px;height:25px; line-height:25px"  /> </td>
                <td></td><td><a href="javascript:void(0)" id="btnQueryData" class="easyui-linkbutton" style="width: 80px;height: 28px;">查询</a></td>
                <td></td><td></td>
            </tr>
        </table>
        </div>
    </div>
    <div style="height: 88%;">        
        <table fit="true" id="tableListBox">
        </table>
    </div>
</div>
</div>

<div id="DivShowImage" style="display: none">
    <form id="DivShowImageForm" class="easyui-form">
    <div style="padding: 10px; background-image='../../Content/images/amg_3.png'" >
        <table>
        <tr><td style=" text-align:center"><small>进场图片</small></td><td style=" text-align:center"><small>出场图片</small></td></tr>
        <tr><td style=" text-align:center; height:100px; width:380px"><img id="EntranceSmallImage" width="380px" height="100px"/></td><td><img id="ExitSmallImage" width="380px" height="100px"/></td></tr>
        <tr><td><img id="EntranceImage" width="380px" height="280px"/></td><td><img id="ExitImage" width="380px" height="280px"/></td></tr>
        </table>
    </div>
    </form>
</div>


