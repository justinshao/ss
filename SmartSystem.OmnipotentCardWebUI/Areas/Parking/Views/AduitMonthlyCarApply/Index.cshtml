@{
    ViewBag.Title = "月租车申请审核";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="/Scripts/DateExtensions.js" type="text/javascript"></script>
<script src="/Scripts/parking/aduitmonthlycarapply.js?v=1" type="text/javascript"></script>
<script src="/Scripts/easyui/tree-searh-extend.js" type="text/javascript"></script>
<div class="easyui-layout" style="height:500px;width:500px" fit="true">
<div region="west" split="true" title="车场信息" style="width: 250px;">
      <div style="margin:5px 0px 5px 5px; text-align:left">
          <input type="text" id="txtSearchTreeContent" style="width:150px" class="my_textbox"/>&nbsp;<input type="button" style="width: 60px;height: 28px" value="搜索" class="easyui-linkbutton" onclick="btnSearchTreeContent()"/>
      </div>
    <ul id="parkingTree"></ul>
</div>
<div region="center" title="月租车申请审核" style="padding:2px; ">
    <div style="margin-bottom: 2px">
        &nbsp;申请人姓名/电话/车牌号：<input class="easyui-textbox" style="width: 100px; height: 25px" id="txtQueryApplyInfo" />
        &nbsp;状态：<select id="sltStatus" class="my_select" style="width:80px">
            <option value="">不限</option>
            <option value="0">申请中</option>
            <option value="1">已通过</option>
            <option value="2">已拒绝</option>
            <option value="3">已取消</option>
        </select>
        &nbsp;<input class="easyui-datetimebox my_datetimebox" type="text" id="txtStartTime" value="@ViewBag.StartTime" style="width:150px;height:25px; line-height:25px"  />
        <input class="easyui-datetimebox my_datetimebox" type="text" id="txtEndTime" value="@ViewBag.EndTime" style="width:150px;height:25px; line-height:25px"  />
        &nbsp; <a href="javascript:void(0)" id="btnQueryData" class="easyui-linkbutton" style="width: 50px;height: 28px">搜索</a>
    </div>
    <div style="height: 95.5%">
        <table fit="true" id="tableData"></table>
    </div>
</div>
</div>
<div id="divMonthlyCarApplyBox" style="display:none">
    <form id="divMonthlyCarApplyBoxForm" class="easyui-form">
    <div title="月租车申请" style="padding: 5px;">
        <input type="hidden" name="RecordID" id="hiddPassedRecordID" />
        <table cellpadding="3" class="table_open_window_edit">
           <tr>
               <td align="right">
                    车牌号：
                </td>
                <td >
                    <span id="spanPlateNo"></span>
                </td>
            </tr>
               <tr>
               <td align="right">
                    申请人姓名：
                </td>
                <td >
                 <span id="spanApplyName"></span>
                </td>
            </tr>
               <tr>
               <td align="right">
                    申请人电话：
                </td>
                <td >
                <span id="spanApplyMoblie"></span>
                </td>
            </tr>
              <tr>
               <td align="right">
                    车位号：
                </td>
                <td >
                <span id="spanPKLot"></span>
                </td>
            </tr>
              <tr>
               <td align="right">
                    家庭地址：
                </td>
                <td >
                <span id="spanFamilyAddress"></span>
                </td>
            </tr>
           
            <tr>
                <td align="right">
                    申请备注：
                </td>
                <td>
                <span id="spanApplyRemark"></span>
                </td>
            </tr>
            <tr>
                <td align="right">
                    申请时间：
                </td>
                <td>
                <span id="spanApplyDateTime"></span>
                </td>
            </tr>
                <tr>
               <td align="right">
                    车类：
                </td>
                <td >
                 <select id="cmbCarTypeID" class="easyui-combobox" name="CarTypeID" editable=false  style="width:200px"> </select>
                </td>
            </tr>
               <tr>
               <td align="right">
                    车型：
                </td>
                <td >
                     <select id="cmbCarModelID" class="easyui-combobox" name="CarModelID" editable=false  style="width:200px"> </select>
                </td>
            </tr>
             <tr>
                <td align="right">
                    车场区域：
                </td>
                <td>
                <input type="hidden" id="hiddAreaIDS" name="AreaIDS" />
                <select id="cmbAreaIDS" class="easyui-combobox"  editable=false  data-options="multiple:true" style="width:200px"> </select>
                <span style="color:Red">*</span>
                </td>

            </tr>
             <tr>
                 <td align="right">
                    车场通道：
                </td>
                <td>
                <input type="hidden" id="hiddGateID" name="GateID" />
                    <select id="cmbGateID" class="easyui-combobox"  editable=false  data-options="multiple:true" style="width:200px"> </select>
                <span style="color:Red">*</span>
                </td>
            </tr>
             <tr>
                <td align="right">
                    原因：
                </td>
                <td>
                 <input id="txtAuditRemark" name="AuditRemark" class="easyui-textbox" data-options="multiline:true" style="width:200px;height:40px">
                </td>
            </tr>
        </table>
    </div>
    </form>
</div>
<div id="divRefusedApplyBox" style="display:none">
    <form id="divRefusedApplyBoxForm" class="easyui-form">
    <div title="月租车申请" style="padding: 5px;">
        <input type="hidden" name="RecordID" id="hiddRefusedRecordID" />
        <table cellpadding="3" class="table_open_window_edit">
             <tr>
                <td align="right">
                    原因：
                </td>
                <td>
                 <input id="txtRefusedAuditRemark" name="AuditRemark" class="easyui-textbox" data-options="multiline:true" style="width:250px;height:60px">
                </td>
            </tr>
        </table>
    </div>
    </form>
</div>

