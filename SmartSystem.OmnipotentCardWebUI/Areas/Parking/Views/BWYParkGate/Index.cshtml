@{
    ViewBag.Title = "外部车场管理";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="/Scripts/DateExtensions.js" type="text/javascript"></script>
<script src="/Scripts/bwy/parkgate.js?v=55" type="text/javascript"></script>
<div class="easyui-layout" style="height:500px;width:500px" fit="true">
    <div style=" margin:5px 0px 5px 5px">
      <span style=" font-size:12px">车场名称</span>： <input class="easyui-textbox" style="width: 130px; height: 25px" id="txtQueryParkName" />
         <span style=" font-size:12px">通道名称</span>：<input class="easyui-textbox" style="width: 130px; height: 25px" id="txtQueryGateName" />
         <span style=" font-size:12px">来源</span>：<select id="sltDataSource" class="my_select" style="width:100px"><option value="">不限</option><option value="0">泊物云</option><option value="1">赛菲姆</option></select>
        &nbsp; <a href="javascript:void(0)" id="btnQueryData" class="easyui-linkbutton" style="width: 50px; height: 28px">搜索</a>
    </div>
    <div style="height: 89%">
        <table fit="true" id="tableData">
        </table>
    </div>
</div>
<div id="divParkGateBox">
    <form id="divParkGateForm" class="easyui-form">
    <div title="编辑通道" style="padding: 20px;">
        <input type="hidden" name="RecordID" id="RecordID" />
        <table cellpadding="3" class="table_open_window_edit">
            <tr>
                <td align="right">
                    车场编号
                </td>
                <td>
                    <input id="txtParkingID" name="ParkingID" required="true" class="easyui-validatebox form-control"/>
                </td>
            </tr>
            <tr>
                <td align="right">
                    车场名称
                </td>
                <td>
                    <input id="txtParkingName" name="ParkingName" required="true" class="easyui-validatebox  form-control"
                        missingmessage="不能为空" />
                </td>
            </tr>
            <tr>
                <td align="right">
                    通道编号
                </td>
                <td>
                    <input id="txtGateID" name="GateID" required="true" class="easyui-validatebox  form-control"
                        missingmessage="不能为空" />
                </td>
            </tr>
            <tr>
                <td align="right">
                    通道名称
                </td>
                <td>
                    <input id="txtGateName" name="GateName" required="true" class="easyui-validatebox  form-control"
                        missingmessage="不能为空" />
                </td>
            </tr>
        </table>
    </div>
    </form>
</div>
<div id="divQRCodeParkNoBox">
    <form id="divQRCodeParkNoBoxForm" class="easyui-form">
    <div title="二维码车场编号" style="padding: 20px;">
        <input type="hidden" name="RecordID"/>
        <table cellpadding="3" class="table_open_window_edit">
            <tr>
                <td align="right">
                    二维码车场编号
                </td>
                <td>
                    <input id="txtParkNo" name="ParkNo" required="true" class="easyui-validatebox form-control"/>
                </td>
            </tr>
        </table>
    </div>
    </form>
</div>
<div id="divDownloadGate" style="display: none">
     <div title="下载二维码" style="padding: 20px;">
         <input type="hidden" id="hiddDownloadRecordID" name="RecordID"/>
            <table class="my_table">
               
                <tbody>
                    <tr style="background-color:#E0FFFF">
			            <td style="width:150px">二维码边长(cm)</td>
			            <td style="width:150px">建议扫描距离(米)</td>
			            <td style="width:150px">下载链接</td>
		            </tr>
		            <tr>
			            <td>8cm</td><td>0.5m</td><td><a href="#" id="btn258" onclick="return btnDownloadQRCode(this,258)">下载</a></td>
		            </tr>
		            <tr>
			            <td>12cm</td><td>0.8m</td><td><a  href="#" id="btn344" onclick="return btnDownloadQRCode(this,344)">下载</a></td>
		            </tr>
                     <tr>
			            <td>15cm</td><td>1m</td><td><a  href="#" id="btn430" onclick="return btnDownloadQRCode(this,430)">下载</a></td>
		            </tr>
                     <tr>
			            <td>30cm</td><td>1.5m</td><td><a  href="#" id="btn860" onclick="return btnDownloadQRCode(this,860)">下载</a></td>
		            </tr>
                     <tr>
			            <td>50cm</td><td>2.5m</td><td><a  href="#" id="btn1280" onclick="return btnDownloadQRCode(this,1280)">下载</a></td>
		            </tr>
	            </tbody>
            </table>
    </div>
</div>
